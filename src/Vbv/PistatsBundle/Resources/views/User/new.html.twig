{% extends 'VbvPistatsBundle:Main:layout.html.twig' %}
{% block main %}
    {% embed 'VbvPistatsBundle:User:layout.html.twig' %}    
        {% block user_main %}
            {{ parent() }}

			    {{ form(form) }}

				<script>
				    $('#list-tab').removeClass('active');
				    $('#new-tab').addClass('active');
				    $('#show-tab').addClass('disable');
				    $('#edit-tab').addClass('disable');
				</script>

        {% endblock %}
    {% endembed %}
{% endblock %}

